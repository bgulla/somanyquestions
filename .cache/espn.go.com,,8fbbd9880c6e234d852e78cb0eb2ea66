	(function($){
		var $headlines = $('#mod-tab-content-headlines ul.headlines li a');
		$headlines.click(function(e){
			var href = $(this).attr('href');
			if(typeof(mboxRecipeName) !== 'undefined') {
				if(mboxRecipeName == "Remove All Icons") {
					if(href.indexOf("autostart=false") < 0) {
						if(href.indexOf("?") > -1) {
							href = href + "&autostart=false";
						} else {
							href = href + "?autostart=false";
						}
					}
				}
			}
			if (e.metaKey || e.ctrlKey) {
				mboxTrack('headlineClick', 'link=' + href);
				$(this).attr("href",href);
				return;
			} else {
				e.preventDefault();
				mboxTrackLink('headlineClick', 'link=' + href, href);
			}
		});
	})(jQuery);
