            $carouselWrapper = jQuery('#headlines-cycle ul'),
            scrollTimeout = 5000,
            jsonObj = [{"playlist":[{"id": 2350797,"type": "watch","thumbnail": "http://a.espncdn.com/combiner/i?img=/espn360/images/wr/ncaa_wrestling/2350797.jpg&w=160&h=90&20150316124221","title": "NCAA Wrestling - Mat 4","content": {"network": "ESPN3","date": "Friday 11&nbsp;a.m. ET","live": "WATCH"}},{"id": 2350798,"type": "watch","thumbnail": "http://a.espncdn.com/combiner/i?img=/espn360/images/wr/ncaa_wrestling/2350798.jpg&w=160&h=90&20150316124326","title": "NCAA Wrestling - Mat 5","content": {"network": "ESPN3","date": "Friday 11&nbsp;a.m. ET","live": "WATCH"}},{"id": 2350796,"type": "watch","thumbnail": "http://a.espncdn.com/combiner/i?img=/espn360/images/wr/ncaa_wrestling/2350796.jpg&w=160&h=90&20150316124021","title": "NCAA Wrestling - Mat 3","content": {"network": "ESPN3","date": "Friday 11&nbsp;a.m. ET","live": "WATCH"}},{"id": 2417223,"type": "watch","thumbnail": "http://a.espncdn.com/combiner/i?img=/espn360/images/bk/ncaaw/2417223.jpg&w=160&h=90&20150319060336","title": "American vs. Iowa","content": {"network": "ESPN3","date": "Friday 2&nbsp;p.m. ET","live": "WATCH"}},{"id": 4359221,"type": "radio","thumbnail": "http://a.espncdn.com/combiner/i?img=/i/espnradio/stations/espn/podcasts/svpr_300.jpg&h=41","title": "SVP & Russillo","content": {"text": "SVP & Russillo - Weekdays 1-4 p.m. ET on ESPN...","network": "radio"}}]}],
            completeOverride = false;

        function buildLi(liObj) {
            var html = '', key;
            if(liObj !== undefined) {
                if(liObj.type == "watch") {
                    html += '<li data-id="' + liObj.id + '" data-network="' + liObj.content.network + '" data-type="watch" data-index="' + currentLiIndex +'"><a href="#"><div class="media"><div class="vid-icon"></div><img src="' + liObj.thumbnail + '" width="73" height="41" alt=""></div><div class="detail"><h4>' + liObj.title +'</h4><p>';
                    for(key in liObj.content) {
                        html += '<span class="' + key + '">' + liObj.content[key] + '</span>';
                    }
                    html += '</p></div></a></li>';
                } else if(liObj.type == "radio") {
                    html += '<li data-id="' + liObj.id + '" data-network="radio" data-type="radio" data-index="' + currentLiIndex +'" class="radio-item"><a href="#"><div class="media"><img src="' + liObj.thumbnail +'" height="41" alt=""></div><div class="detail"><h4>' + liObj.title + '</h4><p>';
                    for(key in liObj.content) {
                        if(key != "network") {
                            html += '<span class="' + key + '">' + liObj.content[key] + '</span>';
                        }
                    }
                    html += '<span class="radio">Listen</span></p></div></a></li>';
                } else if(liObj.type == "custom") {
                    html += '<li data-id="' + liObj.id + '" data-network="' + liObj.content.network + '" data-type="custom" data-index="' + currentLiIndex +'"><a href="#"><div class="media"><div class="vid-icon"></div><img src="' + liObj.thumbnail + '" width="73" height="41" alt=""></div><div class="detail"><h4>' + liObj.title +'</h4><p>';
                    for(key in liObj.content) {
                        if(liObj.content[key] != "null" && liObj.content[key] != null) {
                            html += '<span class="' + key + '">' + liObj.content[key] + '</span>';
                        }
                    }
                    html += '</p></div></a></li>';
                }

                (currentLiIndex == (maxLi-1) || (currentLiIndex == (jsonObj[0].playlist.length-1))) ? (currentLiIndex = 0) : (currentLiIndex++);
            }
            $carouselWrapper.append(html);
        }

        function nextCarousel() {
            if(!completeOverride) {
                allowScrollClick = false;
                timer = window.clearTimeout(timer);
                buildLi(jsonObj[0].playlist[currentLiIndex]);
                $carouselWrapper.animate({
                    'left': '-' + (liWidth)
                }, 1000, function() {
                    $carouselWrapper.find('li').eq(0).remove();
                    $carouselWrapper.css("left", 0);
                    allowScrollClick = true;
                });

                timer = window.setTimeout(function() {
                    nextCarousel();
                }, scrollTimeout);
            }
        }

        function watchRadioCarouselInit() {
            buildLi(jsonObj[0].playlist[currentLiIndex]);

            timer = window.setTimeout(function() {
                nextCarousel();
            }, scrollTimeout);

            jQuery("#headlines-cycle a.next-headline").on('click', function(e) {
                e.preventDefault();
                if(allowScrollClick) {
					window._espntrack = [
						{"linkTrackEvents" : "event33"},
						{"events" : "event33"},
						{"linkTrackVars":"events,prop9,prop12"}
					];
                    anTrackLink(this, 'espn', 'page1_hlcarousel_arrow', currentLiIndex + ':' + jsonObj[0].playlist[currentLiIndex].content.network);
                    nextCarousel();
                }
            });

            jQuery("#headlines-cycle ul")
                .on("click", "li a", function(e) {
                    e.preventDefault();
                    $parentLi = jQuery(this).parent('li');
                    if($parentLi.data("type") == "watch") {
                        if($parentLi.data("id") !== "") {
                            window.open("http://espn.go.com/watchespn/player?addata=watch_hlcarousel" + $parentLi.data("index") + "&id=" + $parentLi.data("id"),"_blank","toolbar=no, scrollbars=no, resizable=no, top=500, left=500, width=1130, height=800");
							window._espntrack = [
								{"linkTrackEvents" : "event33"},
								{"events" : "event33"},
								{"linkTrackVars":"events,prop9,prop12"}
							];

                            anTrackLink(this, 'espn', 'page1_hlcarousel', $parentLi.data("index") + ':' + $parentLi.data("network"));
                        }
                    } else if($parentLi.data("type") == "radio") {
                        if($parentLi.data("id") !== "") {
                            window.open("http://espn.go.com/espnradio/playPopup?_src=headlinecarousel","_blank","toolbar=no, scrollbars=no, resizable=no, top=500, left=500, width=924, height=420");
							window._espntrack = [
								{"linkTrackEvents" : "event33"},
								{"events" : "event33"},
								{"linkTrackVars":"events,prop9,prop12"}
							];
                            anTrackLink(this, 'espn', 'page1_hlcarousel', $parentLi.data("index") + ':' + $parentLi.data("network"));
                        }
                    } else if($parentLi.data("type") == "custom") {
                        if($parentLi.data("id") !== "") {
                            window._espntrack = [
                                {"linkTrackEvents" : "event33"},
                                {"events" : "event33"},
                                {"linkTrackVars":"events,prop9,prop12"}
                            ];
                            anTrackLink(this, 'espn', 'page1_hlcarousel', 'custom:module');
                            window.open($parentLi.data("id"),"_blank","toolbar=no, scrollbars=yes, resizable=yes, top=500, left=500, width=1130, height=800");
                        }
                    }
                })
                .on("mouseenter", "li a", function(e) {
                    timer = window.clearTimeout(timer);
                })
                .on("mouseleave", "li a", function(e) {
                    timer = window.setTimeout(function() {
                        nextCarousel();
                    }, scrollTimeout);
                });
        }
        watchRadioCarouselInit();
    });
</script><div id="mod-tab-content-myheadlines" class="ui-tabs-panel ui-tabs-hide">
<div class="gradient-container mod-myheadlines" data-numtoshow="8"></div>
</div>
