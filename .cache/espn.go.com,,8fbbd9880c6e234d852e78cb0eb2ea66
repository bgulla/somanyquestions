    <script type="text/javascript">	

		jQuery(function() {

			var currentLiIndex = 0,
				liWidth = 913,
				$carouselWrapper = jQuery('#videohaveyouseen ul.carousel'),
				totalLis = $carouselWrapper.children('li').length,
				$currentVideoLi = jQuery("#currentVideoLi");

			function nextCarousel() {
				if(currentLiIndex == totalLis-1) {
					$carouselWrapper.animate({
				    	left: "0"
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_right');
				  	});
				  	currentLiIndex = 0;
				} else {
					currentLiIndex++;
					$carouselWrapper.animate({
				    	left: "-" + (liWidth * currentLiIndex)
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_right');
				  	});
				}
				if($currentVideoLi.length > 0)
					$currentVideoLi.text(currentLiIndex + 1);
			}

			function prevCarousel() {
				if(currentLiIndex === 0) {
					$carouselWrapper.animate({
						left: "-" + ((totalLis - 1) * liWidth)
				  	}, 500, function() {
						anTrackLink(this, 'espn', 'page1videostripe', 'arrow_left');
				  	});
				  	currentLiIndex = totalLis -1;
				} else {
					currentLiIndex--;
					$carouselWrapper.animate({
				    	left: (liWidth * currentLiIndex)*(-1)
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_left');
				  	});
				}
				if($currentVideoLi.length > 0)
					$currentVideoLi.text(currentLiIndex + 1);
			}

			function videoCarouselInit(jsonObj) {
				if($currentVideoLi.length > 0)
					jQuery("#totalVideoLi").text(totalLis);

				jQuery('#videohaveyouseen div.controls a').on('click', function(e) {
					e.preventDefault();
					if(jQuery(this).hasClass('pg-nextBtn'))
						nextCarousel.call(this);
					else if(jQuery(this).hasClass('pg-prevBtn'))
						prevCarousel.call(this);
			    });

			    jQuery('#videohaveyouseen ul.carousel li a').on("click", function(e) {
			    	e.preventDefault();
			    	jsonObj[0].playlist["initialIndex"] = jQuery(this).data('index');
			    	jQuery.publish('launchVideoOverlay', jsonObj);
			    	anTrackLink(this, 'espn', 'page1videostripe', 'video_'+jQuery(this).data('videoid'));
			    });
			}

			videoCarouselInit([{playlist:{title:"Video Playlist",videos:[{id:10822995,thumbnail:"/media/motion/2014/0422/dm_140422_nba_draft_randle_declares/dm_140422_nba_draft_randle_declares.jpg",title:"Randle Chasing NBA Dream",description:"Former Kentucky forward Julius Randle discusses his decision to leave the Wildcats after one season and enter the NBA draft.",caption:"Randle Chasing NBA Dream",footer:"00:00:22"},{id:10821029,thumbnail:"/media/motion/2014/0422/dm_140422_David_Moyes_sacked_by_Manchester_United/dm_140422_David_Moyes_sacked_by_Manchester_United.jpg",title:"United Sacks Moyes",description:"Manchester United confirmed on Tuesday that David Moyes has been dismissed as manager.",caption:"United Sacks Moyes",footer:"00:01:06"},{id:10821285,thumbnail:"/media/motion/2014/0422/dm_140422_nfl_draft_mcshay_hoge_manziel/dm_140422_nfl_draft_mcshay_hoge_manziel.jpg",title:"Evaluating Manziel",description:"Todd McShay and Merril Hoge go to the tape to break down QB Johnny Manziel's weaknesses.",caption:"Evaluating Manziel",footer:"00:04:47"},{id:10821649,thumbnail:"/media/motion/2014/0422/dm_140422_nfl_DraftBlitz_Clowney/dm_140422_nfl_DraftBlitz_Clowney.jpg",title:"Questioning Clowney",description:"Scouts Inc.'s Kevin Weidl and Steve Muench talks about the work ethic questions surrounding NFL Draft prospect Jadeveon Clowney.",caption:"Questioning Clowney",footer:"00:02:36"},{id:10823264,thumbnail:"/media/motion/2014/0422/dm_140422_nfl_espn_to_broadcast_playoff_game/dm_140422_nfl_espn_to_broadcast_playoff_game.jpg",title:"NFL Playoffs On ESPN",description:"Adam Schefter reacts to the news that ESPN will televise a wild-card playoff game in 2015, the first telecast of an NFL postseason game in the network's 35-year history.",caption:"NFL Playoffs On ESPN",footer:"00:00:21"},{id:10822243,thumbnail:"/media/motion/2014/0422/dm_140422_nhl_savard_interview/dm_140422_nhl_savard_interview.jpg",title:"Savard: Time To Get Even",description:"Blue Jackets defenseman David Savard ta