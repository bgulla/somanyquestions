    <script type="text/javascript">	

		jQuery(function() {

			var currentLiIndex = 0,
				liWidth = 913,
				$carouselWrapper = jQuery('#videohaveyouseen ul.carousel'),
				totalLis = $carouselWrapper.children('li').length,
				$currentVideoLi = jQuery("#currentVideoLi");

			function nextCarousel() {
				if(currentLiIndex == totalLis-1) {
					$carouselWrapper.animate({
				    	left: "0"
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_right');
				  	});
				  	currentLiIndex = 0;
				} else {
					currentLiIndex++;
					$carouselWrapper.animate({
				    	left: "-" + (liWidth * currentLiIndex)
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_right');
				  	});
				}
				if($currentVideoLi.length > 0)
					$currentVideoLi.text(currentLiIndex + 1);
			}

			function prevCarousel() {
				if(currentLiIndex === 0) {
					$carouselWrapper.animate({
						left: "-" + ((totalLis - 1) * liWidth)
				  	}, 500, function() {
						anTrackLink(this, 'espn', 'page1videostripe', 'arrow_left');
				  	});
				  	currentLiIndex = totalLis -1;
				} else {
					currentLiIndex--;
					$carouselWrapper.animate({
				    	left: (liWidth * currentLiIndex)*(-1)
				  	}, 500, function() {
				  		anTrackLink(this, 'espn', 'page1videostripe', 'arrow_left');
				  	});
				}
				if($currentVideoLi.length > 0)
					$currentVideoLi.text(currentLiIndex + 1);
			}

			function videoCarouselInit(jsonObj) {
				if($currentVideoLi.length > 0)
					jQuery("#totalVideoLi").text(totalLis);

				jQuery('#videohaveyouseen div.controls a').on('click', function(e) {
					e.preventDefault();
					if(jQuery(this).hasClass('pg-nextBtn'))
						nextCarousel.call(this);
					else if(jQuery(this).hasClass('pg-prevBtn'))
						prevCarousel.call(this);
			    });

			    jQuery('#videohaveyouseen ul.carousel li a').on("click", function(e) {
			    	e.preventDefault();
			    	jsonObj[0].playlist["initialIndex"] = jQuery(this).data('index');
			    	jQuery.publish('launchVideoOverlay', jsonObj);
			    	anTrackLink(this, 'espn', 'page1videostripe', 'video_'+jQuery(this).data('videoid'));
			    });
			}

			videoCarouselInit([{playlist:{title:"Video Playlist",videos:[{id:12522007,thumbnail:"/media/motion/2015/0320/dm_150320_nba_amin_kevindurant/dm_150320_nba_amin_kevindurant.jpg",title:"KD Shut Down For Season?",description:"Amin Elhassan reacts to the news that Kevin Durant will be out indefinitely and figures to not see him the rest of the season.",caption:"KD Shut Down For Season?",footer:"00:02:50"},{id:12523072,thumbnail:"/media/motion/2015/0320/dm_150320_ncb_brennan_on_kansas/dm_150320_ncb_brennan_on_kansas.jpg",title:"Kansas Cruises In Opener",description:"Eamonn Brennan discusses No. 2 seed Kansas' easy victory against No. 15 seed New Mexico State in the Round of 64.",caption:"Kansas Cruises In Opener",footer:"00:01:59"},{id:12521838,thumbnail:"/media/motion/2015/0320/dm_150320_MOBILE_NCB_Must-See_Ron_Hunter_Sound_From_Mike_and_Mike/dm_150320_MOBILE_NCB_Must-See_Ron_Hunter_Sound_From_Mike_and_Mike.jpg",title:"Bilas Fuels Hunter's Fire",description:"Georgia State head coach Ron Hunter joined \"Mike & Mike\" to talk about how ESPN's Jay Bilas' comments are helping his team prepare for the games in the NCAA tournament.",caption:"Bilas Fuels Hunter's Fire",footer:"00:00:44"},{id:12521281,thumbnail:"/media/motion/2015/0320/dm_150320_COM_SC_Feature_Not_Top_Plays/dm_150320_COM_SC_Feature_Not_Top_Plays.jpg",title:"SC: 'Not Top Plays'",description:"Check out the worst plays from the past week.",caption:"SC: 'Not Top Plays'",footer:"00:01:37"},{id:12517259,thumbnail:"/media/motion/2015/0319/dm_150319_SC_BUFFALO_ALL_ACCESS/dm_150319_SC_BUFFALO_ALL_ACCESS.jpg",title:"All-Access: Buffalo",description:"\"SportsCenter\" goes behind the scenes with Buffalo before its NCAA tournament game.",caption:"All-Access: Buffalo",footer:"00:01:56"},{id:12520808,thumbnail:"/media/motion/2015/0320/dm_150320_ncb_bliasgreenberg_biggestsurprise/dm_150320_ncb_bliasgreenberg_biggestsurprise.jpg",title:"Biggest Day 1 Surprises",description:"In a day full of surprises, Seth Greenberg and Jay Bilas both agree that UAB's upset of Iowa State was the biggest shocker of them all.",caption:"Biggest Day 1 Surprises",footer:"00:00:59"},{id:12507170,thumbnail:"/media/motion/2015/0318/dm_150318_ncb_brennan_friday_preview/dm_150318_ncb_brennan_friday_preview.jpg",title:"More Madness Ahead",description:"ESPN.com's Eamonn Brennan previews the best of Friday's NCAA tournament action, including a possible upset in the Midwest.",caption:"More Madness Ahead",footer:"00:02:46"},{id:12521876,thumbnail:"/media/motion/2015/0320/dm_150320_nba_amin_anthonydavis/dm_150320_nba_amin_anthonydavis.jpg",title:"Davis' Return Up In Air",description:"Amin Elhassan explains how Anthony Davis' ankle injury still has his playing status doubtful for Friday's game against the Warriors.",caption:"Davis' Return Up In Air",footer:"00:00:38"},{id:12519800,thumbnail:"/media/motion/2015/0320/dm_150320_LSU_NC_State_Highlight/dm_150320_LSU_NC_State_Highlight.jpg",title:"NC State Wins At Buzzer",description:"BeeJay Anya's leaner in the final seconds was the winner in NC State's 66-65 victory over LSU.",caption:"NC State Wins At Buzzer",footer:"00:00:20"},{id:12519320,thumbnail:"/media/motion/2015/0319/dm_150319_hunter_reenactment/dm_150319_hunter_reenactment.jpg",title:"Hunter Re-Enacts Dad's Fall",description:"Georgia State guard R.J. Hunter, the son of 14th-seeded Panthers head coach Ron Hunter, re-enacted his dad's reaction to the end of GSU's one-point upset of No. 3 Baylor.",caption:"Hunter Re-Enacts Dad's Fall",footer:"00:00:18"},{id:12519847,thumbnail:"/media/motion/2015/0320/dm_150320_Kentucky_Hampton_Highlight/dm_150320_Kentucky_Hampton_Highlight.jpg",title:"Kentucky Puts On Clinic",description:"Karl-Anthony Towns recorded a double-double in Kentucky's 79-56 win over Hampton.",caption:"Kentucky Puts On Clinic",footer:"00:00:20"},{id:12521813,thumbnail:"/media/motion/2015/0320/dm_150320_unpub323_nba_iverson_ft/dm_150320_unpub323_nba_iverson_ft.jpg",title:"FT: Iverson The Exec?",description:"